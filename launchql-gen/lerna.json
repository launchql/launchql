{
  "lerna": "3.4.2",
  "useWorkspaces": true,
  "conventionalCommits": true,
  "npmClient": "yarn",
  "npmClientArgs": ["--no-lockfile"],
  "packages": ["packages/*"],
  "version": "independent",
  "registry": "https://registry.npmjs.org",
  "command": {
    "create": {
      "homepage": "https://github.com/launchql/launchql-gen",
      "license": "SEE LICENSE IN LICENSE",
      "access": "restricted"
    },
    "publish": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "yes": true,
      "verifyAccess": false,
      "message": "chore(release): publish"
    },
    "version": {
      "message": "chore(release): release"
    }
  }
}
