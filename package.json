{
  "name": "launchql",
  "version": "2.0.0",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  },
  "workspaces": [
    "packages/*",
    "sandbox/*",
    "graphile/*"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "clean": "pnpm -r run clean",
    "build": "pnpm -r run build",
    "build:dev": "pnpm -r run build:dev",
    "lint": "pnpm -r run lint",
    "deps": "pnpm up -r -i -L"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/jest-in-case": "^1.0.9",
    "@types/node": "^20.12.7",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "jest": "^30.2.0",
    "jest-in-case": "^1.0.2",
    "lerna": "^8.2.3",
    "prettier": "^3.0.2",
    "rimraf": "6.1.2",
    "strip-ansi": "^6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "graphql": "15.10.1",
    "graphql-upload": "^13.0.0",
    "@pyramation/postgis": "0.1.1"
  },
  "packageManager": "pnpm@9.12.1",
  "pnpm": {
    "publicHoistPattern": [
      "@jest/test-sequencer"
    ],
    "packageExtensions": {
      "jest-config@*": {
        "dependencies": {
          "@jest/test-sequencer": "^29.7.0"
        }
      }
    }
  }
}
