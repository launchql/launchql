    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.6.2",
    "jest-in-case": "^1.0.2",
    "lerna": "^6",
    "prettier": "^3.0.2",
    "@types/rimraf": "^4.0.5",
    "rimraf": "4.4.1",
    "strip-ansi": "^6",
    "symlink-workspace": "^1.9.0",
