{
  "name": "@launchql/server",
  "version": "2.4.0",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "LaunchQL Server",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepare": "npm run build",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "dev": "ts-node src/run.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/run.ts",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "bucket:create": "ts-node src/scripts/create-bucket.ts"
  },
  "keywords": [
    "server",
    "graphql",
    "express",
    "launchql",
    "backend"
  ],
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.2",
    "@launchql/env": "^2.3.0",
    "@launchql/s3-utils": "^2.2.0",
    "@launchql/server-utils": "^2.3.0",
    "@launchql/types": "^2.4.0",
    "@launchql/upload-names": "^2.2.0",
    "@launchql/url-domains": "^2.2.0",
    "@pyramation/postgis": "^0.1.1",
    "@pyramation/postgraphile-plugin-fulltext-filter": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphile-build": "^4.14.1",
    "graphile-cache": "^1.2.0",
    "graphile-i18n": "^0.0.3",
    "graphile-meta-schema": "^0.2.5",
    "graphile-query": "^2.3.0",
    "graphile-search-plugin": "^0.1.2",
    "graphile-settings": "^2.4.0",
    "graphile-simple-inflector": "^0.1.1",
    "graphql-tag": "2.12.6",
    "graphql-upload": "^13.0.0",
    "lru-cache": "^11.1.0",
    "pg": "^8.16.0",
    "pg-query-context": "^2.2.0",
    "postgraphile": "^4.14.1",
    "postgraphile-derived-upload-field": "^0.0.6",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "postgraphile-plugin-connection-filter-postgis": "^1.0.0-alpha.6",
    "request-ip": "^3.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/graphql-upload": "^8.0.12",
    "@types/pg": "^8.15.2",
    "@types/request-ip": "^0.0.41",
    "@types/rimraf": "^4.0.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  },
  "resolutions": {
    "graphql": "15.10.1"
  }
}
