{
  "name": "@launchql/s3-streamer",
  "version": "2.6.5",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "stream files to s3",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "clean": "makage clean",
    "prepack": "npm run build",
    "build": "makage clean && makage build-ts && makage assets",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@launchql/env": "workspace:^",
    "@launchql/s3-utils": "workspace:^",
    "glob": "^13.0.0",
    "makage": "^0.1.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/lib-storage": "^3.600.0",
    "@launchql/content-type-stream": "workspace:^",
    "@launchql/types": "workspace:^"
  },
  "keywords": [
    "s3",
    "stream",
    "upload",
    "aws",
    "minio",
    "launchql"
  ]
}
