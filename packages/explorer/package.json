{
  "name": "@launchql/explorer",
  "version": "2.6.2",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "LaunchQL Explorer",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "bin": {
    "explorer": "./run.js"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepare": "npm run build",
    "build": "npm run clean; tsc -p tsconfig.json; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "dev": "ts-node src/run.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/run.ts",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@launchql/env": "workspace:^",
    "@launchql/s3-streamer": "workspace:^",
    "@launchql/server-utils": "workspace:^",
    "@launchql/types": "workspace:^",
    "@launchql/upload-names": "workspace:^",
    "@launchql/url-domains": "workspace:^",
    "express": "^5.1.0",
    "graphile-build": "^4.14.1",
    "graphile-cache": "workspace:^",
    "graphile-settings": "workspace:^",
    "graphql": "15.10.1",
    "graphql-upload": "^13.0.0",
    "pg-cache": "workspace:^",
    "pg-env": "workspace:^",
    "postgraphile": "^4.14.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/graphql-upload": "^8.0.12",
    "@types/rimraf": "^4.0.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "graphql",
    "explorer",
    "ui",
    "launchql",
    "interface"
  ]
}
