{
  "name": "@launchql/explorer",
  "version": "2.9.0",
  "author": "Constructive <developers@constructive.io>",
  "description": "LaunchQL Explorer",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/constructive-io/constructive",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/constructive-io/constructive"
  },
  "bugs": {
    "url": "https://github.com/constructive-io/constructive/issues"
  },
  "bin": {
    "explorer": "./run.js"
  },
  "scripts": {
    "clean": "makage clean",
    "prepack": "npm run build",
    "build": "makage build",
    "build:dev": "makage build --dev",
    "dev": "ts-node src/run.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/run.ts",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
    "dependencies": {
      "@launchql/env": "workspace:^",
      "@launchql/s3-streamer": "workspace:^",
      "@launchql/types": "workspace:^",
      "@launchql/upload-names": "workspace:^",
      "@launchql/url-domains": "workspace:^",
    "@pgpmjs/server-utils": "workspace:^",
    "@pgpmjs/types": "workspace:^",
    "express": "^5.1.0",
    "graphile-build": "^4.14.1",
    "graphile-cache": "workspace:^",
    "graphile-settings": "workspace:^",
    "graphql": "15.10.1",
    "graphql-upload": "^13.0.0",
    "pg-cache": "workspace:^",
    "pg-env": "workspace:^",
    "postgraphile": "^4.14.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/graphql-upload": "^8.0.12",
    "makage": "^0.1.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "graphql",
    "explorer",
    "ui",
    "launchql",
    "interface"
  ]
}
