{
  "name": "pg-env",
  "version": "1.1.5",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "PostgreSQL environment configuration utilities",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "clean": "makage clean",
    "prepack": "npm run build",
    "build": "makage clean && makage build-ts && makage assets",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "postgres",
    "postgresql",
    "environment",
    "config",
    "launchql"
  ],
  "devDependencies": {
    "makage": "^0.1.5"
  }
}
