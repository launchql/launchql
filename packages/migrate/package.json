{
  "name": "@launchql/migrate",
  "version": "2.2.0",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "LaunchQL Migrate",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "copy": "npm run copy:pkg; npm run copy:sql",
    "copy:pkg": "copyfiles -f ../../LICENSE README.md package.json dist",
    "copy:sql": "copyfiles -f src/sql/* dist/sql && copyfiles -f src/sql/* dist/esm/sql",
    "clean": "rimraf dist/**",
    "prepare": "npm run build",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "dependencies": {
    "@launchql/server-utils": "^2.1.14",
    "@launchql/types": "^2.1.12",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9"
  }
}