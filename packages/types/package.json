{
  "name": "@launchql/types",
  "version": "2.8.4",
  "author": "Dan Lynch <Dan Lynch>",
  "description": "LaunchQL types",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepack": "npm run build",
    "build": "npm run clean; tsc -p tsconfig.json; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "graphile-build": "^4.14.1",
    "pg-env": "workspace:^",
    "postgraphile": "^4.14.1"
  },
  "keywords": [
    "graphql",
    "types",
    "typescript",
    "launchql",
    "schema"
  ]
}
