{
  "name": "@launchql/create-gen-app",
  "version": "0.0.1",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "Clone and customize template repositories",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepack": "npm run build",
    "build": "npm run clean; tsc -p tsconfig.json; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/rimraf": "^4.0.5",
    "strip-ansi": "^6",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "inquirerer": "^2.0.8",
    "rimraf": "^6.0.1"
  }
}
