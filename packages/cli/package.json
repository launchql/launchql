{
  "name": "@launchql/cli",
  "version": "1.4.8",
  "description": "launchql cli",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/launchql/launchql/tree/master/packages/launchql-db#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "bin": {
    "lql": "main/index.js",
    "launchql": "main/index.js"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "scripts": {
    "build:main": "BABEL_ENV=production babel src --out-dir main --delete-dir-on-start",
    "build:module": "MODULE=true babel src --out-dir module --delete-dir-on-start",
    "build": "npm run build:module && npm run build:main",
    "prepare": "npm run build",
    "prebuild": "node ./bin/build.js",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "dev": "NODE_ENV=development babel-node src/index"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@launchql/db-env": "^0.1.4",
    "@launchql/db-migrate": "^1.3.17",
    "@launchql/db-template": "^1.0.2",
    "@launchql/db-templates": "^1.2.1",
    "@launchql/db-testing": "^1.2.4",
    "@launchql/db-transform": "^0.3.5",
    "@launchql/db-utils": "^1.2.1",
    "@launchql/explorer": "^0.12.2",
    "@launchql/server": "^0.21.7",
    "@pyramation/prompt": "0.0.1",
    "case": "1.6.3",
    "glob": "7.1.6",
    "graphile-gen": "^0.3.7",
    "graphql": "^14.0.2",
    "graphql-request": "3.0.0",
    "inflection": "1.12.0",
    "inquirerer": "^0.1.3",
    "introspectron": "0.2.1",
    "minimist": "^1.2.5",
    "mkdirp": "1.0.4",
    "node-walkup": "1.1.1",
    "pg-promise": "10.7.1",
    "pgsql-parser": "1.2.2",
    "shelljs": "0.8.4"
  }
}
