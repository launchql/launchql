{
  "name": "@launchql/core",
  "version": "2.17.6",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "LaunchQL Package and Migration Tools",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/launchql",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "scripts": {
    "copy": "npm run copy:pkg; npm run copy:sql",
    "copy:pkg": "makage assets",
    "copy:sql": "copyfiles -f src/migrate/sql/* dist/migrate/sql && copyfiles -f src/migrate/sql/* dist/esm/migrate/sql && copyfiles -f src/init/sql/* dist/init/sql && copyfiles -f src/init/sql/* dist/esm/init/sql",
    "clean": "makage clean",
    "prepack": "npm run build",
    "build": "makage build && npm run copy",
    "build:dev": "makage build --dev",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "database",
    "migration",
    "postgresql",
    "launchql",
    "schema",
    "sqitch"
  ],
  "devDependencies": {
    "@types/pg": "^8.15.2",
    "copyfiles": "^2.4.1",
    "makage": "^0.1.8"
  },
  "dependencies": {
    "@launchql/env": "workspace:^",
    "@launchql/logger": "workspace:^",
    "@launchql/server-utils": "workspace:^",
    "@launchql/types": "workspace:^",
    "@pgsql/types": "^17.6.1",
    "csv-to-pg": "^2.0.10",
    "glob": "^13.0.0",
    "komoji": "^0.7.8",
    "parse-package-name": "^1.0.0",
    "pg": "^8.16.0",
    "pg-cache": "workspace:^",
    "pg-env": "workspace:^",
    "pgsql-deparser": "^17.12.1",
    "pgsql-parser": "^17.9.1",
    "yanse": "^0.1.5"
  },
  "peerDependencies": {
    "@pgsql/types": "^17.6.1"
  }
}
