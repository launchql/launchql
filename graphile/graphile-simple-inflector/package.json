{
  "name": "graphile-simple-inflector",
  "version": "0.1.3",
  "description": "Simple inflector plugin for Graphile/PostGraphile",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/launchql/launchql",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "makage clean",
    "copy": "makage assets",
    "prepack": "npm run build",
    "build": "makage clean && makage build-ts && makage assets",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "keywords": [
    "graphile",
    "postgraphile",
    "postgres",
    "graphql",
    "inflection",
    "plugin",
    "launchql"
  ],
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "devDependencies": {
    "graphile-test": "workspace:^",
    "graphql-tag": "2.12.6",
    "makage": "^0.1.5",
    "pgsql-test": "workspace:^"
  },
  "dependencies": {
    "graphile-build": "^4.14.1"
  }
}
