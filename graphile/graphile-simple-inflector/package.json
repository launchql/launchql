{
  "name": "graphile-simple-inflector",
  "version": "0.1.2",
  "description": "Simple inflector plugin for Graphile/PostGraphile",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/launchql/launchql",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist/**",
    "prepack": "npm run build",
    "build": "npm run clean; tsc -p tsconfig.json; tsc -p tsconfig.esm.json; npm run copy",
    "build:dev": "npm run clean; tsc -p tsconfig.json --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/launchql"
  },
  "keywords": [
    "graphile",
    "postgraphile",
    "postgres",
    "graphql",
    "inflection",
    "plugin",
    "launchql"
  ],
  "bugs": {
    "url": "https://github.com/launchql/launchql/issues"
  },
  "devDependencies": {
    "graphile-test": "workspace:^",
    "graphql-tag": "2.12.6",
    "pgsql-test": "workspace:^"
  },
  "dependencies": {
    "graphile-build": "^4.14.1"
  }
}
